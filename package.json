{
  "name": "antoni-cv-monorepo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @apps/web dev",
    "build": "pnpm --filter @turbulence/cms build && pnpm --filter @apps/web build",
    "build:cms": "pnpm --filter @turbulence/cms build",
    "start": "pnpm --filter @apps/web start",
    "lint": "pnpm --filter @apps/web lint"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  }
}
